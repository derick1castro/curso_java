import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner	sc = new Scanner(System.in);
			
		
		int num1 = sc.nextInt();
		int entrada = ((num1 < 0) ? 1 : 2);
		
		System.out.println(entrada);
	
	
	
		sc.close();
	}

}
